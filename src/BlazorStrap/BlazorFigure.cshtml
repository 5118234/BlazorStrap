@using Microsoft.AspNetCore.Blazor

<figure class="figure">
    @ChildContent
</figure>

@functions {
    string classname
    {
        get
        {
            return $"figure {Class}";
        }
    }
    public string Class { get; set; }
    public string Style { get; set; }
    public RenderFragment ChildContent { get; set; }
}
