@using Microsoft.AspNetCore.Blazor

<figure class="figure">
    @ChildContent
</figure>

@functions {
    string classname
    {
        get
        {
            return $"figure {Class}";
        }
    }
    [Parameter]
    private string Class { get; set; }
    [Parameter]
    private string Style { get; set; }
    [Parameter]
    private RenderFragment ChildContent { get; set; }
}
